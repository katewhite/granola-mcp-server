{
  "mcpServers": {
    "granola": {
      "command": "node",
      "args": ["/absolute/path/to/granola-mcp-server/index.js"],
      "env": {
        "GRANOLA_API_KEY": "your_granola_api_key_here",
        "GRANOLA_USER_ID": "your_granola_user_id_here",
        "GRANOLA_USER_EMAIL": "your_email@example.com"
      }
    }
  }
}